  "scripts": {
    "dev:sapper": "node ./node_modules/sapper/dist/cli.js dev",
    "build:sapper": "node ./node_modules/sapper/dist/cli.js build --legacy",
    "export": "node ./node_modules/sapper/dist/cli.js export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch:tailwind": "node ./node_modules/postcss-cli/bin/postcss src/tailwind.postcss -o static/main.css -w",
    "build:tailwind": "set NODE_ENV=production && node ./node_modules/postcss-cli/bin/postcss src/tailwind.postcss -o static/main.css",
    "build": "concurrently \"npm run build:tailwind\" \"npm run build:sapper\"",
    "dev": "concurrently \"npm:dev:sapper\" \"npm:watch:tailwind\""
  },
