{
	"hosting": {
		"public": "static",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/graphql",
				"function": "graphqlServer"
			},
			{
				"source": "**",
				"function": "ssr"
			}
		]
	},
	"functions": {
		"source": "."
	}
}
